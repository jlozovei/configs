{
  "color_scheme": "Packages/Babel/Monokai Phoenix.tmTheme",
  "folder_exclude_patterns":
  [
    ".svn",
    ".tmp",
    ".git",
    ".hg",
    "CVS",
    "coverage",
    "vendor",
    "node_modules"
 ],
  "font_size": 14,
  "margin": 2,
  "tab_size": 2,
  "translate_tabs_to_spaces": true,
  "save_on_focus_lost": true,
  "ignored_packages":
  [
    "Markdown",
    "Vintage"
  ],
  "theme": "Seti.sublime-theme"
}
